msgid "rtp2httpd"
msgstr "rtp2httpd"

msgid "rtp2httpd converts multicast RTP/UDP media into http stream. Here you can configure the settings."
msgstr "rtp2httpd converts multicast RTP/UDP media into http stream. Here you can configure the settings."

msgid "rtp2httpd_Enabled"
msgstr "Enabled"

msgid "rtp2httpd_Respawn"
msgstr "Respawn"

msgid "rtp2httpd_Auto restart after crash"
msgstr "Auto restart after crash"

msgid "rtp2httpd_Port"
msgstr "Port"

msgid "rtp2httpd_Verbose"
msgstr "Logging level"

msgid "rtp2httpd_Fatal"
msgstr "Fatal"

msgid "rtp2httpd_Error"
msgstr "Error"

msgid "rtp2httpd_Warn"
msgstr "Warn"

msgid "rtp2httpd_Info"
msgstr "Info"

msgid "rtp2httpd_Debug"
msgstr "Debug"

msgid "rtp2httpd_Advanced Interface Settings"
msgstr "Advanced Interface Settings"

msgid "rtp2httpd_Configure separate interfaces for multicast, FCC and RTSP"
msgstr "Configure separate interfaces for multicast, FCC and RTSP"

msgid "rtp2httpd_Upstream Interface"
msgstr "Upstream Interface"

msgid "rtp2httpd_Default interface for all upstream traffic (multicast, FCC and RTSP). Leave empty to use routing table."
msgstr "Default interface for all upstream traffic (multicast, FCC and RTSP). Leave empty to use routing table."

msgid "rtp2httpd_Upstream Multicast Interface"
msgstr "Upstream Multicast Interface"

msgid "rtp2httpd_Interface to use for multicast (RTP/UDP) upstream media stream (default: use routing table)"
msgstr "Interface to use for multicast (RTP/UDP) upstream media stream (default: use routing table)"

msgid "rtp2httpd_Upstream FCC Interface"
msgstr "Upstream FCC Interface"

msgid "rtp2httpd_Interface to use for FCC unicast upstream media stream (default: use routing table)"
msgstr "Interface to use for FCC unicast upstream media stream (default: use routing table)"

msgid "rtp2httpd_Upstream RTSP Interface"
msgstr "Upstream RTSP Interface"

msgid "rtp2httpd_Interface to use for RTSP unicast upstream media stream (default: use routing table)"
msgstr "Interface to use for RTSP unicast upstream media stream (default: use routing table)"

msgid "rtp2httpd_Max clients"
msgstr "Max clients allowed"

msgid "rtp2httpd_Workers"
msgstr "Workers"

msgid "rtp2httpd_Number of worker processes (SO_REUSEPORT sharded). Set to CPU cores for best perf."
msgstr "Number of worker processes. Set to 1 for resource-constrained devices, or CPU cores for best performance."

msgid "rtp2httpd_Buffer Pool Max Size"
msgstr "Buffer Pool Max Size"

msgid "rtp2httpd_Maximum number of buffers in zero-copy pool. Each buffer is 1536 bytes. Increase to improve throughput for multi-client concurrency."
msgstr "Maximum number of buffers in zero-copy pool. Each buffer is 1536 bytes. Default is 16384 (~24MB). Increase to improve throughput for multi-client concurrency. Not recommended when running behind reverse proxies."

msgid "rtp2httpd_Hostname"
msgstr "Hostname"

msgid "rtp2httpd_Hostname description"
msgstr "When configured, HTTP Host header will be checked and must match this value to allow access. This value is also used as the hostname in M3U playlist conversion. When using a reverse proxy, configure this with the proxied access URL (including http(s):// and path prefix), e.g., https://my-domain.com/rtp2httpd, and ensure the reverse proxy forwards the Host header."

msgid "rtp2httpd_xff"
msgstr "XFF Recognize"

msgid "rtp2httpd_Enable X-Forwarded-For header recognize"
msgstr "Enable X-Forwarded-For header recognize."

msgid "rtp2httpd_R2H Token"
msgstr "R2H Token"

msgid "rtp2httpd_Authentication token for HTTP requests"
msgstr "If set, all HTTP requests must include r2h-token query parameter with matching value (e.g., http://server:5140/service?r2h-token=your-token)"

msgid "rtp2httpd_Use Config File"
msgstr "Use Config File"

msgid "rtp2httpd_Use config file instead of individual options"
msgstr "Use config file instead of individual options"

msgid "rtp2httpd_Config File Content"
msgstr "Config File Content"

msgid "rtp2httpd_Edit the content of /etc/rtp2httpd.conf"
msgstr "Edit the content of /etc/rtp2httpd.conf"

msgid "rtp2httpd_Status Dashboard"
msgstr "Status Dashboard"

msgid "rtp2httpd_Open Status Dashboard"
msgstr "Open Status Dashboard"

msgid "rtp2httpd_Multicast Rejoin Interval"
msgstr "Multicast Rejoin Interval"

msgid "rtp2httpd_Multicast rejoin interval description"
msgstr "Periodic multicast rejoin interval in seconds (0=disabled, default 0). Enable this (e.g., 120-180 seconds) if your network switches timeout multicast memberships due to missing IGMP Query messages. Only needed for problematic network environments."

msgid "rtp2httpd_FCC Listen Port Range"
msgstr "FCC Listen Port Range"

msgid "rtp2httpd_FCC listen port range description"
msgstr "Local UDP port range for FCC client sockets (format: start-end, e.g., 40000-40100). Leave empty to use random ports."

msgid "rtp2httpd_Status Page Path"
msgstr "Status Page Path"

msgid "rtp2httpd_Status page path description"
msgstr "URL path for the status page (default: /status)"

msgid "rtp2httpd_Video Snapshot"
msgstr "Video Snapshot"

msgid "rtp2httpd_Video snapshot description"
msgstr "Enable video snapshot feature. When enabled, clients can request snapshots with snapshot=1 query parameter"

msgid "rtp2httpd_FFmpeg Path"
msgstr "FFmpeg Path"

msgid "rtp2httpd_FFmpeg path description"
msgstr "Path to FFmpeg executable. Leave empty to use system PATH (default: ffmpeg)"

msgid "rtp2httpd_FFmpeg Arguments"
msgstr "FFmpeg Arguments"

msgid "rtp2httpd_FFmpeg arguments description"
msgstr "Additional FFmpeg arguments for snapshot generation. Common options: -hwaccel none, -hwaccel auto, -hwaccel vaapi (for Intel GPU)"

msgid "rtp2httpd_Zero-Copy on Send"
msgstr "Zero-Copy on Send"

msgid "rtp2httpd_Enable zero-copy send with MSG_ZEROCOPY for better performance. Requires kernel 4.14+. Can improve throughput and reduce CPU usage on supported devices."
msgstr "Enable zero-copy send with MSG_ZEROCOPY for better performance. Requires kernel 4.14+ (MSG_ZEROCOPY support). On supported devices, this can improve throughput and reduce CPU usage, especially under high concurrent load."

msgid "rtp2httpd_Player Page"
msgstr "Player Page"

msgid "rtp2httpd_Open Player Page"
msgstr "Open Player Page"

msgid "rtp2httpd_Player Page Path"
msgstr "Player Page Path"

msgid "rtp2httpd_Player page path description"
msgstr "URL path for the player page (default: /player)"

msgid "rtp2httpd_External M3U"
msgstr "External M3U"

msgid "rtp2httpd_External M3U description"
msgstr "Fetch M3U playlist from a URL (file://, http://, https:// supported). Example: https://example.com/playlist.m3u or file:///path/to/playlist.m3u"

msgid "rtp2httpd_External M3U Update Interval"
msgstr "External M3U Update Interval"

msgid "rtp2httpd_External M3U update interval description"
msgstr "External M3U automatic update interval in seconds (default: 86400 = 24 hours). Set to 0 to disable automatic updates."
