msgid "rtp2httpd"
msgstr "rtp2httpd"

msgid "rtp2httpd converts multicast RTP/UDP media into http stream. Here you can configure the settings."
msgstr "rtp2httpd converts multicast RTP/UDP media into http stream. Here you can configure the settings."

msgid "rtp2httpd_Enabled"
msgstr "Enabled"

msgid "rtp2httpd_Respawn"
msgstr "Respawn"

msgid "rtp2httpd_Auto restart after crash"
msgstr "Auto restart after crash"

msgid "rtp2httpd_Port"
msgstr "Port"

msgid "rtp2httpd_Verbose"
msgstr "Logging level"

msgid "rtp2httpd_Quiet"
msgstr "Quiet"

msgid "rtp2httpd_Error"
msgstr "Error"

msgid "rtp2httpd_Warn"
msgstr "Warn"

msgid "rtp2httpd_Info"
msgstr "Info"

msgid "rtp2httpd_Debug"
msgstr "Debug"

msgid "rtp2httpd_Upstream Unicast Interface"
msgstr "Upstream Unicast Interface"

msgid "rtp2httpd_Upstream Multicast Interface"
msgstr "Upstream Multicast Interface"

msgid "rtp2httpd_Interface_multicast to use for requesting multicast upstream media stream (default none, which follows the routing table)"
msgstr "Interface to use for requesting multicast (RTP/UDP) upstream media stream (default none, which follows the routing table)"

msgid "rtp2httpd_Interface_unicast to use for requesting unicast upstream media stream (default none, which follows the routing table)"
msgstr "Interface to use for requesting unicast (FCC/RTSP) upstream media stream (default none, which follows the routing table)"

msgid "rtp2httpd_Max clients"
msgstr "Max clients allowed"

msgid "rtp2httpd_Workers"
msgstr "Workers"

msgid "rtp2httpd_Number of worker processes (SO_REUSEPORT sharded). Set to CPU cores for best perf."
msgstr "Number of worker processes. Set to 1 for resource-constrained devices, or CPU cores for best performance."

msgid "rtp2httpd_Buffer Pool Max Size"
msgstr "Buffer Pool Max Size"

msgid "rtp2httpd_Maximum number of buffers in zero-copy pool. Each buffer is 1536 bytes. Increase to improve throughput for multi-client concurrency."
msgstr "Maximum number of buffers in zero-copy pool. Each buffer is 1536 bytes. Default is 16384 (~24MB). Increase to improve throughput for multi-client concurrency."

msgid "rtp2httpd_Hostname"
msgstr "Hostname"

msgid "rtp2httpd_Hostname to check in the Host: HTTP header"
msgstr "If enabled, rtp2httpd will only allow connections if the 'Host:' HTTP header matches the hostname"

msgid "rtp2httpd_R2H Token"
msgstr "R2H Token"

msgid "rtp2httpd_Authentication token for HTTP requests"
msgstr "If set, all HTTP requests must include r2h-token query parameter with matching value (e.g., http://server:5140/service?r2h-token=your-token)"

msgid "rtp2httpd_FCC NAT traversal"
msgstr "FCC NAT traversal mode"

msgid "rtp2httpd_Only needed when used as a downstream router"
msgstr "Only needed when used as a downstream router"

msgid "rtp2httpd_Don't use NAT traversal"
msgstr "Don't use NAT traversal"

msgid "rtp2httpd_NAT punch hole"
msgstr "NAT punch hole (deprecated, do not use)"

msgid "rtp2httpd_NAT-PMP"
msgstr "NAT-PMP (Requires upstream router support)"

msgid "rtp2httpd_Use Config File"
msgstr "Use Config File"

msgid "rtp2httpd_Use config file instead of individual options"
msgstr "Use config file instead of individual options"

msgid "rtp2httpd_Config File Content"
msgstr "Config File Content"

msgid "rtp2httpd_Edit the content of /etc/rtp2httpd.conf"
msgstr "Edit the content of /etc/rtp2httpd.conf"

msgid "rtp2httpd_Status Dashboard"
msgstr "Status Dashboard"

msgid "rtp2httpd_Open Status Dashboard"
msgstr "Open Status Dashboard"

msgid "rtp2httpd_Multicast Rejoin Interval"
msgstr "Multicast Rejoin Interval"

msgid "rtp2httpd_Multicast rejoin interval description"
msgstr "Periodic multicast rejoin interval in seconds (0=disabled, default 0). Enable this (e.g., 120-180 seconds) if your network switches timeout multicast memberships due to missing IGMP Query messages. Only needed for problematic network environments."

msgid "rtp2httpd_FCC Listen Port Range"
msgstr "FCC Listen Port Range"

msgid "rtp2httpd_FCC listen port range description"
msgstr "Local UDP port range for FCC client sockets (format: start-end, e.g., 40000-40100). Leave empty to use random ports."

msgid "rtp2httpd_Status Page Path"
msgstr "Status Page Path"

msgid "rtp2httpd_Status page path description"
msgstr "URL path for the status page (default: /status)"

msgid "rtp2httpd_Video Snapshot"
msgstr "Video Snapshot"

msgid "rtp2httpd_Video snapshot description"
msgstr "Enable video snapshot feature. When enabled, clients can request snapshots with snapshot=1 query parameter"

msgid "rtp2httpd_FFmpeg Path"
msgstr "FFmpeg Path"

msgid "rtp2httpd_FFmpeg path description"
msgstr "Path to FFmpeg executable. Leave empty to use system PATH (default: ffmpeg)"

msgid "rtp2httpd_FFmpeg Arguments"
msgstr "FFmpeg Arguments"

msgid "rtp2httpd_FFmpeg arguments description"
msgstr "Additional FFmpeg arguments for snapshot generation. Common options: -hwaccel none, -hwaccel auto, -hwaccel vaapi (for Intel GPU)"
